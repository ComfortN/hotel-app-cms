{
    "version": 2,
    "builds": [
      {
        "src": "src/*",
        "use": "@vercel/static-build"
      },
      {
        "src": "backend/server.js",  
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/backend/$1"},
      { "src": "/(.*)", "dest": "/src/$1" }
    ]
  }
  